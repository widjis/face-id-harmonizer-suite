{
  "name": "face-id-harmonizer-backend",
  "version": "1.0.0",
  "description": "Backend API for Face ID Harmonizer Suite with audit trail",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:migrate": "ts-node src/database/migrate.ts migrate",
    "db:seed": "ts-node src/database/migrate.ts seed",
    "db:setup": "ts-node src/database/migrate.ts all",
    "db:reset": "npm run db:setup",
    "test": "jest"
  },
  "keywords": [
    "face-id",
    "vault",
    "audit-trail",
    "batch-processing"
  ],
  "author": "MTI",
  "license": "MIT",
  "dependencies": {
    "@types/xml2js": "^0.4.14",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mssql": "^10.0.1",
    "multer": "^1.4.5-lts.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-validator": "^2.20.33",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mssql": "^9.1.8",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.10.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  }
}
